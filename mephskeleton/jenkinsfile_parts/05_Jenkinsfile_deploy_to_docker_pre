       stage('Deploy docker') {
            when {  expression { return env.GIT_BRANCH.contains('/release/') || env.GIT_BRANCH.contains('/hotfix/') } }
            steps
            {
              withCredentials([usernamePassword(credentialsId: '#cicd.sysops#', usernameVariable: 'CICD_USER', passwordVariable: 'CICD_PASS')])
              {
                //milestone(6)
                  echo 'Desplegamos en PRE'
                  dir('./build_pipeline') {
                      sh './04_deploy.sh -f jenkins -f registrymng_pre -e pre'
                  }
               }
            }
       }
