       stage('Zip lsdomains test') {
            steps
            {
              withCredentials([
                        usernamePassword(credentialsId: '#cicd.sysops.s3.deploy#', usernameVariable: 'CICD_S3_USER', passwordVariable: 'CICD_S3_PASS'),
                        usernamePassword(credentialsId: '#cicd.sysops#', usernameVariable: 'CICD_USER', passwordVariable: 'CICD_PASS')
                        ])
              {
                //milestone(7)
                  echo 'Desplegamos en PRE lsdonains'
                  dir('./build_pipeline') {
                    sh './04_deploy_lsdomains.sh -f jenkins -e test'
                    script
                    {
                        LAST_GROUPNAME = readFile('./tmp/zip_s3_files/LAST_GROUPNAME.txt').trim()
                        LAST_ARTIFACT = readFile('./tmp/zip_s3_files/LAST_ARTIFACT.txt').trim()
                    }
                  }
               }
            }
       }

       stage('Deploy lsdomains test') {
            steps
            {
                //milestone(7)
                echo 'Desplegamos en TEST lsdonains {' +  LAST_GROUPNAME + '} y {' + LAST_ARTIFACT + '}'
                dir('./build_pipeline') {
                    build job:'softbase/utils/deploy_lsdomains2_test', parameters: [[$class: 'StringParameterValue', name: 'GROUPNAME', value: LAST_GROUPNAME],[$class: 'StringParameterValue', name: 'ARTIFACT', value: LAST_ARTIFACT]]
                }
            }
       }
